{
    "name": "motomedialab/laravel-google-signin",
    "type": "library",
    "license": "MIT",
    "description": "Simple implementation for Google oAuth sign-in utilising Laravel Passport",
    "keywords": [
        "motomedialab",
        "google",
        "sso",
        "laravel",
        "socialite"
    ],
    "authors": [
        {
            "name": "Chris Page",
            "email": "chris@motocom.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/socialite": "^v5.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.17",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Motomedialab\\GoogleSignin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Motomedialab\\GoogleSignin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Motomedialab\\GoogleSignin\\GoogleSigninServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
